{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clean": "npm run kill-port && npm run dev",
    "kill-port": "node -e \"require('child_process').exec('netstat -ano | findstr :5173', (err, stdout) => { if (stdout) { const pid = stdout.split(/\\s+/).find(s => /^\\d+$/.test(s)); if (pid) require('child_process').exec(`taskkill /F /PID ${pid}`); } })\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@tanstack/react-query": "^5.90.2",
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "dayjs": "^1.11.18",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/node": "^24.7.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
